# for cachelist[pqnum][rsnum]
# libdpd/init.cc

# i corresponds to one subspace
# pqnum     sym-type
  5*i+0     None             All p,q
  5*i+1     Symmetric        p > q
  5*i+2     Antisymmetric    p > q
  5*i+3     Symmetric        p >= q
  5*i+4     Antisymmetric    p >= q

# if subspace = [occupied, virtual]
0   occ       None             All p,q
1   occ       Symmetric        p > q
2   occ       Antisymmetric    p > q
3   occ       Symmetric        p >= q
4   occ       Antisymmetric    p >= q
5   vir       None             All p,q
6   vir       Symmetric        p > q
7   vir       Antisymmetric    p > q
8   vir       Symmetric        p >= q
9   vir       Antisymmetric    p >= q
10  occ-vir   None             All p,q
11  vir-occ   None             All p,q

# if subspace = [symm-orb, active-orb]
0   so       None             All p,q
1   so       Symmetric        p > q
2   so       Antisymmetric    p > q
3   so       Symmetric        p >= q
4   so       Antisymmetric    p >= q
5   active   None             All p,q
6   active   Symmetric        p > q
7   active   Antisymmetric    p > q
8   active   Symmetric        p >= q
9   active   Antisymmetric    p >= q
